         <record model="ir.cron" id="export_customer_vendor_cron" forcecreate="True">
           <field name="name">Export Customer and Vendor cron</field>
           <field name="interval_number">15</field>
           <field name="interval_type">minutes</field>
           <field eval="False" name="active"/>
