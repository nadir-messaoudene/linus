<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data>
       <record id="product_template_product_form_linus_account" model="ir.ui.view">
            <field name="name">product.template.product.form.linus.account</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_only_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='default_code']" position="after">
                    <field name='country_of_origin_ids' widget="many2many_tags"/>
                </xpath>
            </field>
	    </record>
    </data>
</odoo>